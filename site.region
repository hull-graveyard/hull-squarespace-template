<!DOCTYPE html>
<html lang="en">

  <head>
    
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1">

    <!-- Hull & JQuery -->
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
    <script src="https://d3f5pyioow99x0.cloudfront.net/develop/hull.js"></script>
    
    <!-- Bootstrap -->
    <link href="//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.2/css/bootstrap-combined.min.css" rel="stylesheet">
    <script src="//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.2/js/bootstrap.min.js"></script>
    
    <!-- Hull Init -->
    <script>
      Hull.init({
        appId: "5217633d0fb8a365e000000e",
        orgUrl: "https://153e644e.hullapp.io",
        debug: true //optional
      }, function() {
        console.warn("Hull init ok"); // optional
      });
    </script>

    <!--INCLUDE SQS SCRIPTS, META TAGS AND USER CONTENT FROM THE CODE INJECTION TAB-->
    {squarespace-headers}

    <squarespace:script src="site.js" combo="true" />
  
  </head>

  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">
    

    <div class="container">

      <!--HEADER-->

      <header id="header">

        <!--SITE TITLE OR LOGO-->

        <!--MAIN NAVIGATION-->

        <squarespace:navigation navigationId="mainNav" template="navigation" />

        <div data-hull-component="identity@hull" style="float:right"></div>
      </header>

      <!--CONTENT INJECTION POINT-->

      <section id="content">    
       {squarespace.main-content}
      </section>

      <!--FOOTER WITH OPEN BLOCK FIELD-->

      <footer id="footer">
        <squarespace:block-field id="footerBlocks" columns="12" />
      </footer>

      <!--INJECTION POINT FOR TRACKING SCRIPTS AND USER CONTENT FROM THE CODE INJECTION TAB-->

      {squarespace-footers}
    </div>
  </body>

</html> 